<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.android.musicapp">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".Now_Playing"
            android:label="Now Playing" />
        <activity
            android:name=".Playlist"
            android:label="Playlist" />
        <activity
            android:name=".Artist_Albums"
            android:label="Artists and Albums" />
        <activity
            android:name=".Search"
            android:label="Search" />
        <activity
            android:name=".Search_Online"
            android:label="Search Online" />
        <activity
            android:name=".Play_Online"
            android:label="Play Online" />
        <activity android:name=".Buy_Songs"></activity>
    </application>

</manifest>